.top-tip {
    display: none;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 99999;
}
.top-tip a.close {
    background: url("../images/icons.png") no-repeat scroll -18px -5932px transparent;
    display: block;
    height: 16px;
    position: absolute;
    right: 15px;
    top: 16px;
    width: 16px;
}
.top-tip .top-message {
    border: 1px solid;
    cursor: pointer;
    display: block;
    font-size: 16px;
    margin: 0 auto;
    padding: 30px 8px 30px 45px;
    vertical-align: middle;
}
.top-tip .top-message.error {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: #F2DEDE;
    border-color: -moz-use-text-color #EED4D8 #EED4D8;
    border-image: none;
    border-right: 1px solid #EED4D8;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    color: #AD4740; 
}
.top-tip .top-message.warning {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: #FCF8E2;
    border-color: -moz-use-text-color #FBEFD3 #FBEFD3;
    border-image: none;
    border-right: 1px solid #FBEFD3;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    color: #8B6D37; 
}
.top-tip .top-message.info {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #E0F0D7;
    border-color: -moz-use-text-color #DDECCE #DDECCE;
    border-image: none;
    border-right: 1px solid #DDECCE;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    color: #3F7538;
    font-family: "微软雅黑";
}
.top-tip .top-message.show {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: #D9EDF8;
    border-color: -moz-use-text-color #C7ECF4 #C7ECF4;
    border-image: none;
    border-right: 1px solid #C7ECF4;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    color: #2D709F; 
}
